{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "import/prefer-default-export": "off",
        "indent": "off",
        "linebreak-style": "off",
        "no-multiple-empty-lines": "off",
        "no-unused-vars": "off",
        "space-before-blocks": "off",
        "eol-last": "off",
        "no-extra-semi": "off",
        "no-trailing-spaces": "off",
        "padded-blocks": "off",
        "quotes": "off",
        "import/no-unresolved": "off",
        "camelcase": "off",
        "no-console": "off",
        "import/extensions": "off",
        "class-methods-use-this": "off",
        "max-len": "off"
    }
}